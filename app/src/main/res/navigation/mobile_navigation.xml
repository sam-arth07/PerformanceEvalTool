<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.pet.evaluator.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_resume"
        android:name="com.pet.evaluator.ui.resume.ResumeFragment"
        android:label="Resume"
        tools:layout="@layout/fragment_resume" />

    <fragment
        android:id="@+id/navigation_video"
        android:name="com.pet.evaluator.ui.video.VideoFragment"
        android:label="Video"
        tools:layout="@layout/fragment_video" />

    <fragment
        android:id="@+id/navigation_results"
        android:name="com.pet.evaluator.ui.results.ResultsFragment"
        android:label="Results"
        tools:layout="@layout/fragment_results" />
</navigation>
